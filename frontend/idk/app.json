{
  "expo": {
    "name": "idk",
    "slug": "idk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo/SplashImg.png",
      "resizeMode": "contain",
      "backgroundColor": "#3FB7FF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ssafyidk.idk"
    },
    "web": {
      "favicon": "./assets/logo/app_icon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "MaplestoryBold": ["./assets/fonts/MaplestoryBold.ttf"]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "24087108-d566-43c2-a2b6-fc60d8c108a5"
      }
    }
  }
}
